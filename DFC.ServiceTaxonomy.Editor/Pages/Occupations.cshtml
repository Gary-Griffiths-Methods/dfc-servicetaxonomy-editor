@page
@model DFC.ServiceTaxonomy.Editor.OccupationsModel
@{
    ViewData["Title"] = "Occupations";
}

<h1>Missing Occupations</h1>

<table class="table">
    @foreach (var occupation in Model.MissingOccupations)
    {
    <tr>
        <td>@occupation.JobProfile</td>
        <td>@occupation.EscoTitle</td>
        <td>@occupation.EscoUri</td>
    </tr>
    }
</table>
