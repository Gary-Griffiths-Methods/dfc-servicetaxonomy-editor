    "TitlePart": {
      "Title": "Social worker"
    },
    "OtherRoute": {
      "Description": {
        "Html": "\u003Cp\u003EYou can apply to do an intensive, work-based training programme for graduates if you\u0027ve got a first or upper second class degree in any subject except social work. The programmes are:\u003Ca href=\u0022https://thefrontline.org.uk/\u0022\u003EFrontline\u003C/a\u003E for children\u0027s social work; \u003Ca href=\u0022https://www.gov.uk/guidance/step-up-to-social-work-information-for-applicants\u0022\u003EStep Up to Social Work\u003C/a\u003E for children\u0027s social work; \u003Ca href=\u0022https://thinkahead.org/about-the-programme/how-to-apply/\u0022\u003EThink Ahead\u003C/a\u003E for mental health social work\u003C/p\u003E"
      }
    },
